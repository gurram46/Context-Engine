{
  "name": "context-engine",
  "version": "1.1.1",
  "description": "Context Engine CLI - Compress the Chaos",
  "main": "index.js",
  "bin": {
    "context-engine": "./bin/context-engine.js",
    "ce": "./bin/ce.js"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "npm run build",
    "build": "node scripts/build.js",
    "build:ci": "node scripts/build.js",
    "prepack": "npm run build",
    "postinstall": "node scripts/postinstall.js",
    "clean": "rm -rf dist/ node_modules/.cache/",
    "prepublishOnly": "npm run test && npm run lint",
    "version": "python ../scripts/sync_versions.py $npm_package_version"
  },
  "keywords": [
    "context",
    "compression",
    "cli",
    "documentation",
    "productivity",
    "context-engine"
  ],
  "author": "Context Engine Team",
  "license": "MIT",
  "dependencies": {
    "boxen": "^7.1.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.0.0",
    "cross-spawn": "^7.0.3",
    "figlet": "^1.7.0",
    "gradient-string": "^2.0.2",
    "ink": "^5.0.1",
    "ink-select-input": "^6.1.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^9.2.0",
    "ora": "^5.4.1",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.4",
    "eslint": "^8.50.0",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "bin/",
    "components/",
    "lib/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/contextengine/cli.git"
  },
  "bugs": {
    "url": "https://github.com/contextengine/cli/issues"
  },
  "homepage": "https://contextengine.dev"
}
